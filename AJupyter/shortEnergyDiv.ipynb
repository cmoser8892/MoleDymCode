   "source": [
    "import sympy as sp\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "sp.init_printing()\n",
    "eps = sp.Symbol(\"e\")\n",
    "sig = sp.Symbol(\"s\")\n",
    "rad = sp.Symbol(\"r\")\n",
    "energyRad = 4 * eps * ((sig/rad)**12 - (sig/rad)**6)\n",
    "energyRad.diff(rad)"
   ]
